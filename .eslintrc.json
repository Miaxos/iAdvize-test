{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "indent": ["error", 2],
    "react/jsx-one-expression-per-line": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "max-len": ["warn", 80]
  }
}